{
  "name": "@alpha-video-kit/svg",
  "version": "0.2.5",
  "description": "SVG filter-based renderer for stacked alpha videos",
  "type": "module",
  "files": ["dist", "react.d.ts"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./register": {
      "import": "./dist/register.js",
      "require": "./dist/register.cjs"
    },
    "./react": {
      "types": "./react.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stepancar/alpha-video-kit",
    "directory": "packages/svg"
  }
}
